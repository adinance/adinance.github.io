ed>\n            <input type=\"video\" value=\"https://www.youtube.com/watch?v=9morbKMwCu4\" title=\"Video\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container-fluid\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12\">\n                <h2 class=\"mbr-section-title pb-2 mbr-bold mbr-fonts-style align-left\" mbr-theme-style=\"display-2\" mbr-if=\"showMaintitle\" data-app-selector=\".mbr-section-title, .line-wrap\">Lessons</h2>\n                <div class=\"line-wrap\" mbr-if=\"showMaintitle&&showLineMaintitle\">\n                    <div class=\"line\"></div>\n                </div>\n\n            </div>\n        </div>\n        <div class=\"row row-content justify-content-center\">\n            <div class=\"card p-3 col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4', 'col-lg-4': cardsAmount < '4'}\" mbr-if=\"cardsAmount > 0\">\n                <div class=\"card-wrapper\">\n                    <div class=\"card-img\">\n                        <div class=\"img-hover\" mbr-if=\"showbuttons\">\n                        </div>\n                        <img src=\"../_images/carousel/2.jpg\" title>\n                        <div class=\"img-btn mbr-section-btn\" mbr-buttons mbr-theme-style=\"display-4\" mbr-if=\"showbuttons\"><a href=\"https://mobiri.se\" class=\"btn btn-primary\" data-app-placeholder=\"Type Text\">View Webcasts</a>\n                            <a href=\"https://mobiri.se\" class=\"btn btn-white-outline\" data-app-placeholder=\"Type Text\">Download Presentation</a></div>\n                    </div>\n                    <div class=\"content-box\">\n                        <div class=\"date-box\" mbr-if=\"showdata\">\n                            <h3 class=\"date mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".date\">\n                                23</h3>\n                            <h3 class=\"mounth mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mounth\">\n                                April\n                            </h3>\n                            <h3 class=\"time mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".time\">\n                                5:00pm\n                            </h3>\n                        </div>\n                        <div class=\"card-box\">\n                            <h4 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                Introductory Lesson</h4>\n                            <div class=\"ico-line\" mbr-if=\"showlector\">\n                                <div mbr-icon class=\"px-1 pb-3 pr-2 mbr-iconfont mbr-iconfont-social mbri-user\" style=\"color: #232323;\"></div>\n                                <h3 class=\"rev mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".rev\">\n                                    <font>James Wolf</font>\n                                </h3>\n                            </div>\n\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card p-3 col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4', 'col-lg-4': cardsAmount < '4'}\" mbr-if=\"cardsAmount > 1\">\n                <div 